package java06.InheritanceEx06.Ex01;

public class CodeMain {
	final static int MAXPOKE = 10;
	
	public static void main(String [] args){
		L2Char l2c = new L2Char();
		
		l2c.setName("능력자");
		l2c.setRace("인간");
		
		String ret = "";
		ret += l2c.getName();
		ret += "\n";
		ret += l2c.getRace();
		ret += "\n";
		System.out.println(ret);
		l2c.showAbility();
		
		
		L2Char[] pokemon = new L2Char[MAXPOKE];
		
		
		
		
	}
}
