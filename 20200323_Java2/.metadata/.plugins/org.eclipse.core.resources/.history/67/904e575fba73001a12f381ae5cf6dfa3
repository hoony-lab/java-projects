package java07.AccessController;

public class ArithmeticEx01 {
	final int NUM = 100;
	int result;
	
	public void NoExceptionCase() {
		for (int i = 0; i < 10; i++) {
			try {
				result = NUM/ (int)(Math.random()*10);
			} catch (Exception e) {
				result = 0;
//				System.out.println(e);
			}
			System.out.println(result);
		}
	}
}
