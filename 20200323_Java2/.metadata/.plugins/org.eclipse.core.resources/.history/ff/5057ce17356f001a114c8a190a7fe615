package cls.app;



class Computer {
	String name;
	String spec;
	int ram;
	int hdd;

	Computer() {
	}

	Computer(String n, String s) {
	}

	void setComputer(String n, int r, int h) {
		this.name = n;
		this.ram = r;
		this.hdd = h;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSpec() {
		return spec;
	}

	public void setSpec(String spec) {
		this.spec = spec;
	}

	public int getRam() {
		return ram;
	}

	public void setRam(int ram) {
		this.ram = ram;
	}

	public int getHdd() {
		return hdd;
	}

	public void setHdd(int hdd) {
		this.hdd = hdd;
	}
}


class Member{
	String name;
	String picture;
	
	public Member(String name, String picture) {
		super();
		this.name = name;
		this.picture = picture;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPicture() {
		return picture;
	}
	public void setPicture(String picture) {
		this.picture = picture;
	}
	
}


class ChatBox {

	String picture;
	String id;
	String content;
	String time;

	public ChatBox(String picture, String id, String content, String time) {
		super();
		this.picture = picture;
		this.id = id;
		this.content = content;
		this.time = time;
	}

	public String getPicture() {
		return picture;
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}
}

class InputBox {
	String Chat;

	public String getChat() {
		return Chat;
	}

	public void setChat(String chat) {
		Chat = chat;
	}
}

public class ClassTest {

	public static void main(String[] args) {

		Computer com = new Computer();

		com.setComputer("Ak", 16, 1024);
		
		
		
		Member member = new Member();
		
	}
}
