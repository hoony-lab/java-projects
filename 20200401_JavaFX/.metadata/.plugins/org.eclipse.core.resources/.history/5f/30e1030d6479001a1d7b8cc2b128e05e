//		addIdnPwd("asdf3", "asdf3");
//		addIdnPwd("asdf4", "asdf4");
//		addIdnPwd("asdf5", "asdf5");

		this.idnPwd = idnPwd;
	}

	public MyDB() {
		this(null);
	}

	public HashMap<String, String> getIdnPwd() {
		return idnPwd;
	}

	public void setIdnPwd(HashMap<String, String> idnPwd) {
		this.idnPwd = idnPwd;
	}

	public void addIdnPwd(String id, String pwd) {
		idnPwd.put(id, pwd);
	}
	

